    <script src="https://kit.fontawesome.com/a314b80a3a.js" crossorigin="anonymous"></script>
    <script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
    <title>Apus Tarot Shop – Cửa hàng bán bài Tarot chính hãng tại Hà Nội</title>
</head>
<body>
    <div class="app">
        <!--HEADER-->
        {{> header}}
        {{{body}}}
        {{> footer}}
    </div>
    <script src="/javascript/slider.js"></script>
    <script src="/javascript/main.js"></script>
    <script>
        var haschild = document.getElementById('haschild');
        haschild.innerHTML =    `{{#each typeproduct}}
                                    <li class="header-menu-child__item">
                                        <a href="{{this.link}}" class="header-menu-child__link">
                                            <span>{{this.name}}</span>
                                        </a>
                                    </li>
                                {{/each}}`;
    </script>
</body>
</html>